{
  "name": "Github Super Search",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Popup github search just like you love <3",
  "content_scripts": [
	{ "matches": ["http://*.github.com/*","https://*.github.com/*" ], "js": ["jquery.2.0.3.min.js", "github_super_search.js"], "css": ["github_super_search.css"] }
  ],
  "permissions": [
    "http://www.github.com/"
  ],
  "icons": {
  		    "16": "icon16.png",
          "48": "icon48.png",
          "128": "icon128.png"
  }
}